  "repository": {
    "type": "git",
    "url": "git@git.code.oa.com:wxweb/weflow.git"
  },
  "scripts": {
    "build": "npm run build-js && npm run build-ts",
    "build-js": "cross-env NODE_ENV=production babel src --extensions '.ts' --config-file ../../babel.config.js -d lib --ignore \"src/**/*.test.js\"",
    "build-ts": "node ../../scripts/tsc --rootDir src --outDir lib --declaration --emitDeclarationOnly 'src/**/*.ts'",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "dependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-loader": "^8.1.0"
  }
}
